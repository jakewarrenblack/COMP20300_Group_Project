<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="424.0" prefWidth="220.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.examplefx.HelloController">
   <children>
      <VBox layoutX="10.0" layoutY="21.0">
         <children>
            <Text fx:id="textLabel" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter some text" />
            <TextField fx:id="textField" prefHeight="25.0" prefWidth="116.0" />
            <Button fx:id="okBtn" mnemonicParsing="false" onAction="#onHelloButtonClick" text="OK" />
            <Button fx:id="clear" mnemonicParsing="false" onAction="#onClearButtonClick" text="CLEAR" />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="You entered" />
            <TextArea fx:id="textArea" prefHeight="300.0" prefWidth="200.0" />
         </children>
      </VBox>
   </children>
</AnchorPane>

